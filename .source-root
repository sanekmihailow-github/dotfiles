if [ -d ~/.shell_source ]; then

     source $HOME/.shell_source/exports      && echo 'export loaded'
     source $HOME/.shell_source/defaults     && echo 'default loaded'
     source $HOME/.shell_source/alias        && echo 'alias loaded'
     source $HOME/.shell_source/prompt_root  && echo 'root loaded'
     source $HOME/.shell_source/functions    && echo 'function load'
    #source /home/$homeuser/.shell_source/git

fi
